// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Category {
  id          String   @id @unique @default(uuid())
  name        String
  description String
  createdAt   DateTime
}

model Specification {
  id          String   @id @unique @default(uuid())
  name        String
  description String
  createdAt   DateTime
}
